<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>Followings</h1>
      <ul>
        <% @followings.each do |following| %>
          <li>
            <%= link_to following.name, user_path(following) %>  |
            <%= following.email %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>


