<div class="container">
  <div class="row">
    <div class="col-12">
      <div class="row show-posts">
        <% if @posts.present? %>
          <% @posts.each do |post|%>
            <div class="col-8">
              <%=link_to post.user.username, user_posts_path(post.user_id) %>
              posted:<br/>
              <%=link_to (image_tag post.image_url(:large)), user_post_path(post.user_id,post.id) %>
              <%= post.content %><br/>
            </div>
          <% end %>
        <% else %>
          <div class="side-right">No post found in subscriber</div>
        <% end %>
      </div>
    </div>
  </div>
</div>
