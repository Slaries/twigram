<div class="container">
  <div class="row">
    <div class="col-12">

    </div>
    <div class="row show-posts">
      <% @posts.each do |post|%>
        <div class="col-4">
          <b><%= post.user.username %></b> posted:<br/>
          <%=link_to (image_tag post.image_url(:small)), user_post_path(post.user_id,post.id) %>
          <%= post.content %><br/>
        </div>
      <% end %>
    </div>
  </div>
</div>